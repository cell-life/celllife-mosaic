	<queryString>
		<![CDATA[SELECT
 count(pp_court.id) as 'prog_provided_court', -- Court support
 count(cont_counselling.id) as 'cont_counselling', -- Containment Counselling
 count(education_DVA.id) as 'education_DVA', -- Education and information on domestic violence and abuse
 count(info_court_proc.id) as 'info_court_proc', -- Information about Court Processes and Procedures
 count(referrals_SP.id) as 'referrals_SP', -- Referrals to other Service Providers as needed
 count(info_SRH.id) as 'info_SRH', -- Information on Sexual Reproductive Health
 count(info_HIV_test.id) as 'info_HIV_test', -- Information on HIV and where to be tested

 count(pp_social.id) as 'prog_provided_social_services', -- Social Services
 count(public_awareness.id) as 'public_awareness', -- Public Awareness - Gender Based Violence/HIV/Sexual Reproductive Health & Rights
 count(family_couns_DV.id) as 'family_couns_DV', -- Individual, Couple and Family counselling for victims of Domestic Violence and their families
 count(educational_WS.id) as 'educational_WS', -- Educational workshops
 count(educational_SG.id) as 'educational_SG', -- Educational Support Groups
 count(comm_outreach_events.id) as 'comm_outreach_events', -- Community Outreach Events
 count(advocacy_and_lobbying.id) as 'advocacy_and_lobbying', -- Advocacy and Lobbying

 count(pp_sexual_reprod.id) as 'prog_provided_sexual_reprod_health',  -- Sexual and reproductive health
 count(SexualServHct.id) as 'SexualServHct', -- HCT (HIV Counselling and Testing)

 count(pp_sexual_violence.id) as 'prog_provided_sexual_violence', -- Sexual Violence
 count(containment_couns_SV.id) as 'containment_couns_SV', -- Containment counselling of victims of sexual violence
 count(awareness_raising_act.id) as 'awareness_raising_act', -- Awareness raising activities
 count(pep_info_adherence.id) as 'pep_info_adherence', -- PEP Information and Adherence
 count(thuthuzela_info.id) as 'thuthuzela_info', -- Thuthuzela Care Centre process information

 count(pp_mencarep.id) as 'prog_provided_mencarep', -- Mencare
 count(men_counselling.id) as 'men_counselling', -- Counselling
 count(group_education.id) as 'group_education', -- Group education with young men on SRHR, gender equality and caregiving
 count(comm_campaigns.id) as 'comm_campaigns' -- Community campaigns/awareness on mencare

FROM mosaic_visit_form_v1 mvf1

-- court services
    LEFT JOIN
      (SELECT id from mosaic_visit_form_v1 where ProgProvided like '%court_sup_prog%') as pp_court
      ON pp_court.id = mvf1.id
    LEFT JOIN
      (SELECT id from mosaic_visit_form_v1 where CourtServ like '%cont_counselling%') as cont_counselling
      ON cont_counselling.id = mvf1.id
    LEFT JOIN
      (SELECT id from mosaic_visit_form_v1 where CourtServ like '%education_DVA%') as education_DVA
      ON education_DVA.id = mvf1.id
    LEFT JOIN
      (SELECT id from mosaic_visit_form_v1 where CourtServ like '%info_court_proc%') as info_court_proc
      ON info_court_proc.id = mvf1.id
    LEFT JOIN
      (SELECT id from mosaic_visit_form_v1 where CourtServ like '%referrals_SP%') as referrals_SP
      ON referrals_SP.id = mvf1.id
    LEFT JOIN
      (SELECT id from mosaic_visit_form_v1 where CourtServ like '%info_SRH%') as info_SRH
      ON info_SRH.id = mvf1.id
    LEFT JOIN
      (SELECT id from mosaic_visit_form_v1 where CourtServ like '%info_HIV_test%') as info_HIV_test
      ON info_HIV_test.id = mvf1.id

-- social services
    LEFT JOIN
      (SELECT id from mosaic_visit_form_v1 where ProgProvided like '%social_serv_prog%') as pp_social
      ON pp_social.id = mvf1.id
    LEFT JOIN
      (SELECT id from mosaic_visit_form_v1 where SocialServ like '%public_awareness%') as public_awareness
      ON public_awareness.id = mvf1.id
    LEFT JOIN
      (SELECT id from mosaic_visit_form_v1 where SocialServ like '%family_couns_DV%') as family_couns_DV
      ON family_couns_DV.id = mvf1.id
    LEFT JOIN
      (SELECT id from mosaic_visit_form_v1 where SocialServ like '%educational_WS%') as educational_WS
      ON educational_WS.id = mvf1.id
    LEFT JOIN
      (SELECT id from mosaic_visit_form_v1 where SocialServ like '%educational_SG%') as educational_SG
      ON educational_SG.id = mvf1.id
    LEFT JOIN
      (SELECT id from mosaic_visit_form_v1 where SocialServ like '%comm_outreach_events%') as comm_outreach_events
      ON comm_outreach_events.id = mvf1.id
    LEFT JOIN
      (SELECT id from mosaic_visit_form_v1 where SocialServ like '%advocacy_and_lobbying%') as advocacy_and_lobbying
      ON advocacy_and_lobbying.id = mvf1.id

-- sexual & reproductive health
    LEFT JOIN
      (SELECT id from mosaic_visit_form_v1 where ProgProvided like '%SRH_prog%') as pp_sexual_reprod
      ON pp_sexual_reprod.id = mvf1.id
    LEFT JOIN
      (SELECT id from mosaic_visit_form_v1 where SexualServ like '%SexualServHct%') as SexualServHct
      ON SexualServHct.id = mvf1.id

-- sexual violence
    LEFT JOIN
      (SELECT id from mosaic_visit_form_v1 where ProgProvided like '%SV_prog%') as pp_sexual_violence
      ON pp_sexual_violence.id = mvf1.id
    LEFT JOIN
      (SELECT id from mosaic_visit_form_v1 where SexualViolenceServ like '%containment_couns_SV%') as containment_couns_SV
      ON containment_couns_SV.id = mvf1.id
    LEFT JOIN
      (SELECT id from mosaic_visit_form_v1 where SexualViolenceServ like '%awareness_raising_act%') as awareness_raising_act
      ON awareness_raising_act.id = mvf1.id
    LEFT JOIN
      (SELECT id from mosaic_visit_form_v1 where SexualViolenceServ like '%pep_info_adherence%') as pep_info_adherence
      ON pep_info_adherence.id = mvf1.id
    LEFT JOIN
      (SELECT id from mosaic_visit_form_v1 where SexualViolenceServ like '%thuthuzela_info%') as thuthuzela_info
      ON thuthuzela_info.id = mvf1.id


-- mencare
    LEFT JOIN
      (SELECT id from mosaic_visit_form_v1 where ProgProvided like '%mencare_prog%') as pp_mencarep
      ON pp_mencarep.id = mvf1.id
    LEFT JOIN
      (SELECT id from mosaic_visit_form_v1 where MencServ like '%men_counselling%') as men_counselling
      ON men_counselling.id = mvf1.id
    LEFT JOIN
      (SELECT id from mosaic_visit_form_v1 where MencServ like '%group_education%') as group_education
      ON group_education.id = mvf1.id
    LEFT JOIN
      (SELECT id from mosaic_visit_form_v1 where MencServ like '%comm_campaigns%') as comm_campaigns
      ON comm_campaigns.id = mvf1.id


WHERE 
  mvf1.gender in ('male','female')
  and mvf1.MaritalStatus in ('married', 'single', 'living_together','widowed', 'divorced')
  and mvf1.location in ('wynberg','bellv','khayel','atlantis', 'paarl','welling','mitchels_p')
  and (mvf1.age < 21 or (mvf1.age >= 21 and mvf1.age <= 40)
    or (mvf1.age >= 41 and mvf1.age <=65)
    or (mvf1.age > 65))
  and date between $P{start_date} and $P{end_date};]]>
	</queryString>
