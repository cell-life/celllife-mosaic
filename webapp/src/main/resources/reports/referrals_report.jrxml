	<queryString>
		<![CDATA[SELECT
  attendence.PatRefCode as 'client_ref',
 attendence.referredBy as 'referredBy',
  -- mosaic clinic
 (attendence.visitIntReferral like '%mosaic_clinic%') as 'mosaic_clinic_referred', -- 1=referred 0=n/a
 (attendence.refIntReferral like '%mosaic_clinic%') as 'mosaic_clinic_visited', -- 1=attended, 0/null=not attended
 -- mosaic counselling
 (attendence.visitIntReferral like '%mosaic_counselling%') as 'mosaic_counselling_referred',
 (attendence.refIntReferral like '%mosaic_counselling%') as 'mosaic_counselling_visited',
 -- hiv test / hct
 (attendence.visitIntReferral like '%hiv_test_hct%') as 'hiv_test_hct_referred',
 (attendence.refIntReferral like '%hiv_test_hct%') as 'hiv_test_hct_visited',
-- workshop
 (attendence.visitIntReferral like '%workshop%') as 'workshop_referred',
 (attendence.refIntReferral like '%workshop%') as 'workshop_visited',
  -- support group
  (attendence.visitIntReferral like '%support_group%') as 'support_group_referred',
  (attendence.refIntReferral like '%support_group%') as 'support_group_visited',
-- mencare
 (attendence.visitIntReferral like '%mencare%') as 'mencare_referred',
 (attendence.refIntReferral like '%mencare%') as 'mencare_visited'
FROM
 (
SELECT mvf2.PatRefCode, mvf2.refBy as referredBy, mvf2.IntReferral as visitIntReferral, GROUP_CONCAT(mrf2.IntReferral, ' ') as refIntReferral
FROM mosaic_mosaic_visit_form_v2 mvf2
  LEFT JOIN mosaic_mosaic_referral_form_v2 mrf2 on
    (((mrf2.IdNum = mvf2.IdNum) or (mrf2.PassNum = mvf2.PassNum))
    or ((mrf2.IdNum is null or mvf2.IdNum is null) and (mrf2.PassNum is null or mvf2.PassNum is null) and (mrf2.PatRefCode = mvf2.PatRefCode)))
WHERE
mvf2.date between $P{start_date} and $P{end_date}
GROUP BY mvf2.id
 ) as attendence;]]>
	</queryString>
