	<queryString>
		<![CDATA[SELECT
  attendence.PatRefCode as 'client_ref',
 -- court support
 (attendence.allProgProvided like '%court_sup_prog%') as 'court_support_visit', -- 1=referred 0=n/a
 (attendence.allRefBy like '%court_support%') as 'court_support_referral', -- 1=attended, 0/null=not attended
 -- social services
 (attendence.allProgProvided like '%social_serv_prog%') as 'social_serv_visit',
 (attendence.allRefBy = 'social_serv') as 'social_serv_referral',
 -- sexual & reproductive health
 (attendence.allProgProvided like '%SRH_prog%') as 'SRH_visit',
 (attendence.allRefBy = 'SRH') as 'SRH_referral',
-- sexual violence
 (attendence.allProgProvided like '%SV_prog%') as 'SV_visit',
 (attendence.allRefBy = 'SVP') as 'SV_referral',
-- mencare+
 (attendence.allProgProvided like '%mencare_prog%') as 'mencare_visit',
 (attendence.allRefBy = 'mencare') as 'mencare_referral'
FROM
 (
SELECT mvf1.PatRefCode, mvf1.ProgProvided as allProgProvided, GROUP_CONCAT(mrf1.RefBy, ' ') as allRefBy
FROM mosaic_visit_form_v1 mvf1
  LEFT JOIN mosaic_referral_form_v1 mrf1 on
    (((mrf1.IdNum = mvf1.IdNum) or (mrf1.PassNum = mvf1.PassNum))
       or ((mrf1.IdNum = null or mvf1.IdNum = null) and (mrf1.PassNum = null or mvf1.PassNum = null) and (mrf1.PatRefCode = mvf1.PatRefCode)))
WHERE
 mvf1.date between $P{start_date} and $P{end_date}
GROUP BY mvf1.id
 ) as attendence;]]>
	</queryString>
